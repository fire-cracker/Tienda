  "scripts": {
    "test": "NODE_ENV=test PORT=3001 nyc mocha --require @babel/register tests/**/*.js --exit",
    "lint": "eslint",
    "start": "NODE_ENV=development nodemon --exec babel-node index.js --preset-env",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
